public class MakerMaker{
  public static void main(String [] args){
    System.out.println("public class ProgramMaker{");
      System.out.println("  public static void main(String [] args){");
      System.out.println("  System.out.println(\"public class AddressMaker{\");");
      System.out.println("  System.out.println(\"  public static void main(String [] args){\")");
      System.out.println("  System.out.println(\"    System.out.println(\"Seguin, TX       78155\")\");");
      System.out.println("  System.out.println(\"  }\");");
      System.out.println("  System.out.println(\"}\");");
      System.out.println("  }");
      System.out.println("}");
                           }
}
